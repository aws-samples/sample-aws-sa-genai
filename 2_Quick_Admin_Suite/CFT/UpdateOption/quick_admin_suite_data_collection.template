        Name: pythonshell
        PythonVersion: '3.9'
        ScriptLocation: s3://admin-console-cfn-dataprepare-code/glue/scripts/2025/adminconsolefolderinfo.py
      DefaultArguments:
        '--AWS_REGION': !Sub ${AWS::Region}
        '--S3_OUTPUT_PATH': !Sub s3://admin-suite-${AWS::AccountId}/monitoring/quicksight/folder_assets
      GlueVersion: '3.0'
