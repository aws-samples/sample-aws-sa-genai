      Timeout: 120
    DependsOn:
      - adminconsolenew
  etljobadminsuiteqaccess:
    Type: AWS::Glue::Job
    Properties:
      Command:
        Name: glueetl
        PythonVersion: '3'
        ScriptLocation: !Sub s3://admin-console-cfn-dataprepare-code/glue/scripts/2025/adminconsoleqobjectaccessinfo.py
      DefaultArguments:
        '--region': !Sub ${AWS::Region}
        '--aws_account_id': !Sub ${AWS::AccountId}
        '--output_s3_path': !Sub s3://admin-suite-${AWS::AccountId}/monitoring/quicksight/q_object_access
      GlueVersion: '5.0'
      ExecutionProperty:
        MaxConcurrentRuns: 1
      MaxRetries: 0
      Name: etl_job_admin_suite_q_access
      Role: !GetAtt QuickSightAdminConsole2025.Arn
      Timeout: 120
      WorkerType: G.1X
      NumberOfWorkers: 2
    DependsOn:
      - adminconsolenew
  etljobadminsuiteqmetadata:
    Type: AWS::Glue::Job
